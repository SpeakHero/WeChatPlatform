@model IEnumerable<CQCMXY.WeiXin.Data.Models.MenusButtonAction>

@{
    ViewBag.Title = "列表首页";
}

<h2>列表首页</h2>

<p>
    @Html.ActionLink("新建", "Create")
</p>
<table class="table">
    <tr>
     
        <th>
            @Html.DisplayNameFor(model => model.ActionType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actiondes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActionDetails)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ActionType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actiondes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActionDetails)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("详细信息", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
