@model IEnumerable<CQCMXY.WeiXin.Data.Models.AppTokenInfo>

@{
    ViewBag.Title = "公众号列表";
}

<h2>公众号列表</h2>

<p>
    @Html.ActionLink("添加公众号", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AppTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.appID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.appsecret)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AppTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.appID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.appsecret)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.Id }) |
            @Html.ActionLink("详细", "Details", new { id=item.Id }) |
            @Html.ActionLink("删除", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
